// Coloque su configuración en este archivo para sobrescribir la configuración predeterminada y de usuario.
{
    "clang.cflags": [
        "-Wall",
        "-Wextra",
        "-Werror",
        "-std=gnu11",
        "-I",
        "${workspaceRoot}/include"
    ],
    "clang.cxxflags": [
        "-Wall",
        "-Wextra",
        "-Werror",
        "-std=gnu++11",
        "-I",
        "${workspaceRoot}/include"
    ],
    "editor.formatOnSave": true,
    "C_Cpp.intelliSenseEngine": "Default",
    "files.associations": {
        "*.ipp": "c",
        "stdexcept": "cpp"
    }
}